<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"


    >

    <RelativeLayout
        android:id="@+id/rr_header"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:background="@drawable/bg_login">

        <ImageView
            android:id="@+id/img_hamburger"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_15sdp"
            android:layout_marginTop="25dp"
            android:src="@drawable/ic_humberger" />

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="25dp"
            android:fontFamily="@font/poppins_semibold"
            android:text="My Luggage"
            android:textColor="@color/white"
            android:textSize="@dimen/_14sdp" />

        <ImageView
            android:id="@+id/img_notification"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="@dimen/_15sdp"
            android:src="@drawable/ic_notifications" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/card_view_detail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="70dp"
        android:background="@drawable/drawbale_cardview_corner">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_18sdp"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_18sdp"
                android:layout_marginBottom="@dimen/_14sdp"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/ll_img"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/drawable_round_corner_luggae">

                        <ImageView
                            android:id="@+id/img_suitcase_one"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingStart="@dimen/_11sdp"
                            android:paddingTop="@dimen/_5sdp"
                            android:paddingEnd="@dimen/_11sdp"
                            android:paddingBottom="@dimen/_5sdp"
                            android:src="@drawable/ic_suitcase_one" />
                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/ll_desc"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_9sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_toStartOf="@+id/tv_active_one"
                        android:layout_toEndOf="@+id/ll_img"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins_medium"
                                android:text="Subscription"
                                android:textColor="#A9A9A9"
                                android:textSize="@dimen/_8sdp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_3sdp"
                                android:fontFamily="@font/poppins_medium"
                                android:text="TN001-34"
                                android:textColor="#252525"
                                android:textSize="@dimen/_18sdp" />


                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:src="@drawable/ic_claim_warranty" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_5sdp"
                                    android:fontFamily="@font/poppins_regular"
                                    android:text="@string/claim_warranty"
                                    android:textColor="#FF2200"
                                    android:textSize="@dimen/_8sdp" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_active_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="Active"
                        android:textColor="#E81F00"
                        android:textSize="@dimen/_8sdp" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:layout_marginBottom="@dimen/_15sdp"
                    android:background="#EDEDED" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/ll_img_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/drawable_round_corner_luggae">

                        <ImageView
                            android:id="@+id/img_suitcase_two"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingStart="@dimen/_11sdp"
                            android:paddingTop="@dimen/_5sdp"
                            android:paddingEnd="@dimen/_11sdp"
                            android:paddingBottom="@dimen/_5sdp"
                            android:src="@drawable/ic_suitcase_two" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_desc_two"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_9sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_toStartOf="@+id/tv_active_two"
                        android:layout_toEndOf="@+id/ll_img_two"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins_medium"
                                android:text="Subscription"
                                android:textColor="#A9A9A9"
                                android:textSize="@dimen/_8sdp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_3sdp"
                                android:fontFamily="@font/poppins_medium"
                                android:text="TN001-35"
                                android:textColor="#252525"
                                android:textSize="@dimen/_18sdp" />


                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:src="@drawable/ic_claim_warranty" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_5sdp"
                                    android:fontFamily="@font/poppins_regular"
                                    android:text="@string/claim_warranty"
                                    android:textColor="#FF2200"
                                    android:textSize="@dimen/_8sdp" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_active_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="Active"
                        android:textColor="#E81F00"
                        android:textSize="@dimen/_8sdp" />
                </RelativeLayout>


                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:layout_marginBottom="@dimen/_15sdp"
                    android:background="#EDEDED" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/ll_img_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/drawable_round_corner_luggae">

                        <ImageView
                            android:id="@+id/img_suitcase_three"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingStart="@dimen/_11sdp"
                            android:paddingTop="@dimen/_5sdp"
                            android:paddingEnd="@dimen/_11sdp"
                            android:paddingBottom="@dimen/_5sdp"
                            android:src="@drawable/ic_suitcase_three" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_desc_three"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_9sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_toStartOf="@+id/tv_active_three"
                        android:layout_toEndOf="@+id/ll_img_three"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins_medium"
                                android:text="Subscription"
                                android:textColor="#A9A9A9"
                                android:textSize="@dimen/_8sdp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_3sdp"
                                android:fontFamily="@font/poppins_medium"
                                android:text="TN001-36"
                                android:textColor="#252525"
                                android:textSize="@dimen/_18sdp" />


                        </LinearLayout>
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_active_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="Buy Subscription"
                        android:textColor="#A9A9A9"
                        android:textSize="@dimen/_8sdp" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:layout_marginBottom="@dimen/_15sdp"
                    android:background="#EDEDED" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/ll_img_four"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/drawable_round_corner_luggae">

                        <ImageView
                            android:id="@+id/img_suitcase_four"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingStart="@dimen/_11sdp"
                            android:paddingTop="@dimen/_5sdp"
                            android:paddingEnd="@dimen/_11sdp"
                            android:paddingBottom="@dimen/_5sdp"
                            android:src="@drawable/ic_suitcase_four" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_desc_four"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_9sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_toStartOf="@+id/tv_active_four"
                        android:layout_toEndOf="@+id/ll_img_four"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/poppins_medium"
                                android:text="Subscription"
                                android:textColor="#A9A9A9"
                                android:textSize="@dimen/_8sdp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_3sdp"
                                android:fontFamily="@font/poppins_medium"
                                android:text="TN001-37"
                                android:textColor="#252525"
                                android:textSize="@dimen/_18sdp" />


                        </LinearLayout>
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_active_four"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:fontFamily="@font/poppins_medium"
                        android:text="Buy Subscription"
                        android:textColor="#A9A9A9"
                        android:textSize="@dimen/_8sdp" />
                </RelativeLayout>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btn_add_new_suitcase"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_18sdp"

                    android:background="@drawable/drawable_signup_btn"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Add New Suitcase"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_12sdp" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btn_warranty_log"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/drawable_signup_btn"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Warranty Log"
                    android:textAllCaps="false"

                    android:textColor="@color/white"
                    android:textSize="@dimen/_12sdp" />
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</RelativeLayout>